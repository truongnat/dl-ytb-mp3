{
    "name": "electron-fluent-ui",
    "description": "Template for developing Electron applications using TypeScript and Fluent UI React components",
    "version": "4.4.3",
    "license": "MIT",
    "bin": {
        "ydm": "./cli.js"
    },
    "main": "dist-main/index.js",
    "scripts": {
        "build": "vite build",
        "dev": "vite",
        "lint": "eslint .",
        "package:publish": "electron-builder --config electron-builder.config.js --publish always",
        "package": "electron-builder --config electron-builder.config.js",
        "prettier:check": "prettier --check -- src/**/*.{ts,tsx}",
        "prettier:write": "prettier --write -- src/**/*.{ts,tsx}",
        "test:coverage": "vitest run --coverage",
        "test:watch": "vitest",
        "test": "vitest run",
        "typecheck": "npm run typecheck:main && npm run typecheck:preload && npm run typecheck:renderer",
        "typecheck:main": "tsc -p src/main/tsconfig.json --noEmit",
        "typecheck:preload": "tsc -p src/preload/tsconfig.json --noEmit",
        "typecheck:renderer": "tsc -p src/renderer/tsconfig.json --noEmit"
    },
    "devDependencies": {
        "@eslint/js": "^9.12.0",
        "@types/eslint__js": "^8.42.3",
        "@types/node": "^20.16.11",
        "@types/react": "^18.3.11",
        "@types/react-dom": "^18.3.0",
        "@types/react-router-dom": "^5.3.3",
        "@vitejs/plugin-react": "^4.3.2",
        "@vitest/coverage-v8": "^2.1.2",
        "electron": "^32.2.0",
        "electron-builder": "^24.13.3",
        "eslint": "^9.12.0",
        "globals": "^15.11.0",
        "prettier": "^3.3.3",
        "prettier-plugin-organize-imports": "^4.1.0",
        "typescript": "^5.6.3",
        "typescript-eslint": "^8.8.1",
        "vite": "^5.4.8",
        "vite-plugin-electron": "^0.28.8",
        "vite-plugin-electron-renderer": "^0.14.6",
        "vitest": "^2.1.2"
    },
    "dependencies": {
        "@fluentui/react-components": "^9.55.0",
        "@fluentui/react-icons": "^2.0.260",
        "@ybd-project/ytdl-core": "^6.0.1",
        "axios": "^1.7.7",
        "cheerio": "^1.0.0",
        "commander": "^12.1.0",
        "electron-store": "^10.0.0",
        "fluent-ffmpeg": "^2.1.3",
        "fs-extra": "^11.2.0",
        "https-proxy-agent": "^7.0.5",
        "playwright": "^1.48.0",
        "querystring": "^0.2.1",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-router": "^6.26.2",
        "react-router-dom": "^6.26.2",
        "slugify": "^1.6.6",
        "socks-proxy-agent": "^8.0.4",
        "user-agents": "^1.1.325"
    }
}
